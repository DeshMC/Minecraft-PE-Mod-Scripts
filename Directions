try {
	minigunSpin.reset();
	minigunSpin.setDataSource(sdcard + "/games/com.mojang/desnoguns-sounds/MinigunSpin.ogg");
	minigunSpin.setLooping(true);
	minigunSpin.prepareAsync();
} catch(e) { /* sounds not installed */ }


